<div class="col-sm-12 col-md-6 col-lg-3">
  <div id="card<%= item.gpin %>" class="card text-white bg-success mb-3" style="max-width: 20rem;">
      <h4 class="card-header"> <%= item.name %> <span class="badge badge-light"><%= item.type %> </h4>
      <div class="card-body">
        <p class="card-text">
          Device is <%= item.status %>
        </p>
      </div>
  </div>
</div>
